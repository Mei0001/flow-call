{
  "name": "flow-call",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "cf:build": "npx --yes @cloudflare/next-on-pages@1",
    "cf:preview": "npm run cf:build && npx --yes wrangler pages dev .vercel/output/static --compatibility-flag=nodejs_compat",
    "cf:deploy": "npm run cf:build && npx --yes wrangler pages deploy .vercel/output/static --project-name $CLOUDFLARE_PAGES_PROJECT"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "next": "15.5.3",
    "openai": "^5.20.3",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "reactflow": "^11.11.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.5.3",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
